type Query {
  AutoPolicyByPhoneNumber(id: ID): Auto
  ElectronicsPolicyByPhoneNumber(phoneNumber: String): Electronics
}

type Auto {
  id: ID
  phoneNumber: String
  planName: String
  policyNumber: String
  registrationNumber: String
}

type Electronics {
  phoneNumber: String
  planName: String
  policyNumber: String
  productCategory: String
}